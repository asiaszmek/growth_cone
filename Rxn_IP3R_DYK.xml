        <Reactant specieID="CKCamCa2"  power="3"        />
        <Product  specieID="CKCamCa2"  power="2"         />
        <Product  specieID="CKpCamCa2"           />

        <forwardRate> 1.e-12 </forwardRate>
        <reverseRate> 0e-3     </reverseRate>
        <Q10>     2             </Q10>
    </Reaction>

<!--2c)  2 CKpCamCa2 + 2 CKCamCa2 <-> 3 CKpCamCa2 + 1 CKCamCa2-->
    <Reaction name = "CKCam bind2" id="CKCam_bind2">
        <Reactant specieID="CKpCamCa2"  power="2"        />
        <Reactant specieID="CKCamCa2"  power="2"        />
        <Product  specieID="CKCamCa2"           />
