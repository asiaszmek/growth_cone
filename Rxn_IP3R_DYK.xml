    	<forwardRate>0.0008e-3</forwardRate>
    	<reverseRate>0.01e-3</reverseRate>
    	<Q10>0.2</Q10>
    </Reaction>


 <!--1) CamCa4 + CK <-> CKCam  -->
    <Reaction name = "CK_bind" id="CK_bind">
	<Reactant specieID="CamCa4"/>
