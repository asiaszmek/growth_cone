        <reverseRate> 0e-3     </reverseRate>
        <Q10>     2             </Q10>
    </Reaction>


<!-- CKpCam + CamCa 2<-> CKp -->
    <Reaction name = "CKp1_auto" id="CKp1_auto">
    	<Reactant specieID="CKpCamCa2"/>
        <Product specieID="CamCa2"/>
